<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.account.AccountNewController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="424.0" prefWidth="343.0" spacing="20.0">
         <children>
            <Label text="Create New Account">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <ComboBox fx:id="accountTypeCB" onAction="#accountTypeCBAction" prefHeight="25.0" prefWidth="306.0" promptText="Account Type" />
            <ComboBox fx:id="accountCategoryCB" onAction="#accountCategoryCBAction" prefHeight="25.0" prefWidth="309.0" promptText="Account Category" />
            <TextField fx:id="accountNameTF" promptText="Account Name" />
            <TextField fx:id="accountNumberTF" promptText="Account Number" />
            <TextField fx:id="taxCodeTF" promptText="Default Tax Code" />
            <TextField fx:id="balanceTF" promptText="Balance" />
            <HBox prefHeight="100.0" prefWidth="60.0" spacing="50.0">
               <children>
                  <Button fx:id="okayButton" mnemonicParsing="false" onAction="#okayButtonAction" prefHeight="32.0" prefWidth="72.0" text="Okay">
                     <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../icons/button/ok_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonAction" prefHeight="32.0" prefWidth="84.0" text="Cancel">
                     <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../icons/button/cancel_icon.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
